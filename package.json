{
  "name": "csv-digester",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run genhash",
    "lint": "eslint .",
    "preview": "vite preview",
    "genhash": "node scripts/genhash.mjs",
    "electron": "electron .",
    "start": "npm run build && npm run electron",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "primeicons": "^7.0.0",
    "primereact": "^10.9.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3"
  },
  "build": {
    "appId": "com.CsvDigester",
    "productName": "CsvDigester",
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "directories": {
      "output": "dist-electron"
    }
  }
}
